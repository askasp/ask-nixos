# Amino API environment variables
# Copy this file to amino-api.env and encrypt with agenix

# Database connection
DATABASE_URL=postgres://user:password@localhost:5432/amino

# API settings
PORT=5150
HOST=0.0.0.0
LOG_LEVEL=info

# Authentication
JWT_SECRET=your_jwt_secret_here
AUTH_TOKEN_EXPIRY=86400

# Other configuration
API_KEY=your_api_key_here 